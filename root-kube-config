apiVersion: v1
clusters:
- cluster:
# 定义cluster的认证信息
    certificate-authority-data: 
# 定义cluster的apiserver 的信息
    server: https://192.168.61.133:6443
# 定义集群的名字，后边的上下文(context)会用到
  name: kubernetes
# 上下文(可以定义多个)
contexts:
- context:
    # 使用上边定义好的集群中
    cluster: kubernetes
    user: admin
  # 定义cluster名字
  name: kubernetes
# 定义当前使用的上下文
current-context: kubernetes
kind: Config
preferences: {}
users:
  # 定义user
- name: admin
  user:
    # 使用的证书和key
    client-certificate-data: 
    client-key-data: 
